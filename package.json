{
  "name": "crowdship-interface",
  "version": "0.1.0",
  "private": false,
  "scripts": {
    "dev": "yarn contracts:compile && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "contracts:compile": "typechain --target ethers-v5 --out-dir src/types/contracts './src/abis/**/*.json'",
